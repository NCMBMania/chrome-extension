{
  "name": "NCMB Extension",
  "description": "NCMB Extension supports your data management on NCMB.",
  "version": "1.4",
  "manifest_version": 3,
	"permissions": [],
  "action": {
		"default_icon": "icon16.png"
  },
	"run_at": "document_end",
	"icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
	},
	"content_scripts": [
		{
			"matches": ["https://console.mbaas.nifcloud.com/*"],
			"js": ["js/url.min.js", "js/jquery.min.js", "js/content.js"]
		}
	]
}